	LPH_NO_VIRTUALIZE(function()
		local old;
old = hookmetamethod(game,"__namecall",function(self,...)
local args = {...}
if self.Name == "LogKick" then 
return
end
return old(self, ...)
end)


		for _, Connection in next, getconnections(game:GetService("ScriptContext").Error) do
			Connection:Disable()
